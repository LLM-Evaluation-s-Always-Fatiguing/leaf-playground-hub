{
    "name": "general_mcq_examine",
    "version": "0.1.0",
    "metadata": {
        "scene_metadata": {
            "scene_definition": {
                "name": "GeneralMCQExamine",
                "description": "A general multiple choices questioning examine scene that uses dataset from huggingface hub to test agents.",
                "env_vars": [],
                "roles": [
                    {
                        "name": "examiner",
                        "description": "the one that participants in an multiple choices question examine to monitor the examinees",
                        "actions": [],
                        "num_agents_range": [
                            1,
                            1
                        ],
                        "is_static": true
                    },
                    {
                        "name": "examinee",
                        "description": "the one that participants in an multiple choices question examine to answer questions",
                        "actions": [
                            {
                                "name": "answer_question",
                                "description": "answering the question sent by examiner",
                                "signature": {
                                    "parameters": [
                                        {
                                            "name": "self",
                                            "annotation": "<class 'inspect._empty'>"
                                        },
                                        {
                                            "name": "question",
                                            "annotation": "<class 'general_mcq_examine.scene_definition.ExaminerQuestion'>"
                                        },
                                        {
                                            "name": "examiner",
                                            "annotation": "<class 'leaf_playground.data.profile.Profile'>"
                                        }
                                    ],
                                    "return_annotation": "<class 'general_mcq_examine.scene_definition.ExamineeAnswer'>",
                                    "is_static_method": false
                                },
                                "metrics": [
                                    {
                                        "name": "accurate",
                                        "description": "accuracy of examinee's answer",
                                        "record_value_dtype": "BOOLEAN",
                                        "record_display_type": "BooleanRadio",
                                        "expect_resp_msg_type": "ExamineeAnswer",
                                        "agg_method": {
                                            "fn": {
                                                "obj": "accuracy_fn",
                                                "module": "general_mcq_examine.scene_definition",
                                                "source_file": null
                                            },
                                            "default_kwargs": null
                                        },
                                        "is_comparison": false
                                    }
                                ]
                            }
                        ],
                        "num_agents_range": [
                            1,
                            -1
                        ],
                        "is_static": false
                    }
                ]
            },
            "config_schema": {
                "$defs": {
                    "AnswerQuestionActionConfig": {
                        "properties": {
                            "metrics_config": {
                                "$ref": "#/$defs/AnswerQuestionActionMetricsConfig"
                            }
                        },
                        "required": [
                            "metrics_config"
                        ],
                        "title": "AnswerQuestionActionConfig",
                        "type": "object"
                    },
                    "AnswerQuestionActionMetricsConfig": {
                        "properties": {
                            "accurate": {
                                "$ref": "#/$defs/MetricConfig"
                            }
                        },
                        "required": [
                            "accurate"
                        ],
                        "title": "AnswerQuestionActionMetricsConfig",
                        "type": "object"
                    },
                    "DatasetConfig": {
                        "properties": {
                            "path": {
                                "title": "Path",
                                "type": "string"
                            },
                            "split": {
                                "title": "Split",
                                "type": "string"
                            },
                            "question_column": {
                                "title": "Question Column",
                                "type": "string"
                            },
                            "num_questions": {
                                "default": -1,
                                "title": "Num Questions",
                                "type": "integer"
                            },
                            "golden_answer_column": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Golden Answer Column"
                            },
                            "filter_conditions": {
                                "anyOf": [
                                    {
                                        "items": {
                                            "$ref": "#/$defs/DynamicFn"
                                        },
                                        "type": "array"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Filter Conditions"
                            },
                            "question_preprocessor": {
                                "anyOf": [
                                    {
                                        "$ref": "#/$defs/DynamicFn"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null
                            },
                            "name": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Name"
                            },
                            "data_dir": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Data Dir"
                            },
                            "data_files": {
                                "anyOf": [
                                    {
                                        "items": {
                                            "type": "string"
                                        },
                                        "type": "array"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Data Files"
                            }
                        },
                        "required": [
                            "path",
                            "split",
                            "question_column"
                        ],
                        "title": "DatasetConfig",
                        "type": "object"
                    },
                    "DynamicFn": {
                        "properties": {
                            "fn": {
                                "$ref": "#/$defs/DynamicObject"
                            },
                            "default_kwargs": {
                                "anyOf": [
                                    {
                                        "type": "object"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Default Kwargs"
                            }
                        },
                        "required": [
                            "fn"
                        ],
                        "title": "DynamicFn",
                        "type": "object"
                    },
                    "DynamicObject": {
                        "properties": {
                            "obj": {
                                "title": "Obj",
                                "type": "string"
                            },
                            "module": {
                                "anyOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Module"
                            },
                            "source_file": {
                                "anyOf": [
                                    {
                                        "format": "file-path",
                                        "type": "string"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Source File"
                            }
                        },
                        "required": [
                            "obj"
                        ],
                        "title": "DynamicObject",
                        "type": "object"
                    },
                    "ExamineeRoleActionsConfig": {
                        "properties": {
                            "answer_question": {
                                "$ref": "#/$defs/AnswerQuestionActionConfig"
                            }
                        },
                        "required": [
                            "answer_question"
                        ],
                        "title": "ExamineeRoleActionsConfig",
                        "type": "object"
                    },
                    "ExamineeRoleConfig": {
                        "properties": {
                            "actions_config": {
                                "$ref": "#/$defs/ExamineeRoleActionsConfig"
                            },
                            "agents_config": {
                                "items": {
                                    "$ref": "#/$defs/RoleAgentConfig"
                                },
                                "minItems": 1,
                                "title": "Agents Config",
                                "type": "array"
                            },
                            "is_static": {
                                "const": false,
                                "default": false,
                                "title": "Is Static"
                            }
                        },
                        "required": [
                            "actions_config",
                            "agents_config"
                        ],
                        "title": "ExamineeRoleConfig",
                        "type": "object"
                    },
                    "ExaminerRoleActionsConfig": {
                        "properties": {},
                        "title": "ExaminerRoleActionsConfig",
                        "type": "object"
                    },
                    "ExaminerRoleConfig": {
                        "properties": {
                            "actions_config": {
                                "$ref": "#/$defs/ExaminerRoleActionsConfig"
                            },
                            "agents_config": {
                                "default": [
                                    {
                                        "config_data": {},
                                        "obj_for_import": {
                                            "module": "general_mcq_examine.agents.examiner",
                                            "obj": "Examiner",
                                            "source_file": null
                                        }
                                    }
                                ],
                                "items": {
                                    "$ref": "#/$defs/RoleAgentConfig"
                                },
                                "maxItems": 1,
                                "minItems": 1,
                                "title": "Agents Config",
                                "type": "array"
                            },
                            "is_static": {
                                "const": true,
                                "default": true,
                                "title": "Is Static"
                            }
                        },
                        "required": [
                            "actions_config"
                        ],
                        "title": "ExaminerRoleConfig",
                        "type": "object"
                    },
                    "GeneralmcqexamineSceneRolesConfig": {
                        "properties": {
                            "examiner": {
                                "$ref": "#/$defs/ExaminerRoleConfig"
                            },
                            "examinee": {
                                "$ref": "#/$defs/ExamineeRoleConfig"
                            }
                        },
                        "required": [
                            "examiner",
                            "examinee"
                        ],
                        "title": "GeneralmcqexamineSceneRolesConfig",
                        "type": "object"
                    },
                    "MetricConfig": {
                        "properties": {
                            "enable": {
                                "default": true,
                                "title": "Enable",
                                "type": "boolean"
                            }
                        },
                        "title": "MetricConfig",
                        "type": "object"
                    },
                    "RoleAgentConfig": {
                        "properties": {
                            "config_data": {
                                "title": "Config Data",
                                "type": "object"
                            },
                            "obj_for_import": {
                                "$ref": "#/$defs/DynamicObject"
                            }
                        },
                        "required": [
                            "config_data",
                            "obj_for_import"
                        ],
                        "title": "RoleAgentConfig",
                        "type": "object"
                    }
                },
                "properties": {
                    "env_vars_config": {
                        "const": null,
                        "default": null,
                        "title": "Env Vars Config"
                    },
                    "roles_config": {
                        "$ref": "#/$defs/GeneralmcqexamineSceneRolesConfig"
                    },
                    "dataset_config": {
                        "$ref": "#/$defs/DatasetConfig"
                    }
                },
                "required": [
                    "roles_config",
                    "dataset_config"
                ],
                "title": "GeneralmcqexamineSceneConfig",
                "type": "object"
            },
            "obj_for_import": {
                "obj": "GeneralMCQExamineScene",
                "module": "general_mcq_examine.scene",
                "source_file": null
            }
        },
        "agents_metadata": {
            "examiner": [
                {
                    "cls_name": "Examiner",
                    "description": "An agent who minitor the examine",
                    "config_schema": null,
                    "obj_for_import": {
                        "obj": "Examiner",
                        "module": "general_mcq_examine.agents.examiner",
                        "source_file": null
                    }
                }
            ],
            "examinee": [
                {
                    "cls_name": "OpenAIBasicExaminee",
                    "description": "Examinee agent using OpenAI API to answer questions",
                    "config_schema": {
                        "$defs": {
                            "OpenAIBackendConfig": {
                                "properties": {
                                    "model": {
                                        "enum": [
                                            "babbage-002",
                                            "davinci-002",
                                            "gpt-3.5-turbo-instruct",
                                            "text-davinci-003",
                                            "text-davinci-002",
                                            "text-davinci-001",
                                            "code-davinci-002",
                                            "text-curie-001",
                                            "text-babbage-001",
                                            "text-ada-001",
                                            "gpt-4-1106-preview",
                                            "gpt-4-vision-preview",
                                            "gpt-4",
                                            "gpt-4-0314",
                                            "gpt-4-0613",
                                            "gpt-4-32k",
                                            "gpt-4-32k-0314",
                                            "gpt-4-32k-0613",
                                            "gpt-3.5-turbo",
                                            "gpt-3.5-turbo-16k",
                                            "gpt-3.5-turbo-0301",
                                            "gpt-3.5-turbo-0613",
                                            "gpt-3.5-turbo-16k-0613"
                                        ],
                                        "title": "Model",
                                        "type": "string"
                                    },
                                    "api_key": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "title": "Api Key"
                                    },
                                    "organization": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "title": "Organization"
                                    },
                                    "base_url": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "title": "Base Url"
                                    },
                                    "azure_endpoint": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "title": "Azure Endpoint"
                                    },
                                    "azure_deployment": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "title": "Azure Deployment"
                                    },
                                    "api_version": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "title": "Api Version"
                                    },
                                    "is_azure": {
                                        "default": false,
                                        "title": "Is Azure",
                                        "type": "boolean"
                                    },
                                    "max_retries": {
                                        "default": 2,
                                        "title": "Max Retries",
                                        "type": "integer"
                                    },
                                    "timeout": {
                                        "default": 60,
                                        "title": "Timeout",
                                        "type": "number"
                                    }
                                },
                                "required": [
                                    "model"
                                ],
                                "title": "OpenAIBackendConfig",
                                "type": "object"
                            },
                            "Profile": {
                                "properties": {
                                    "id": {
                                        "title": "Id",
                                        "type": "string"
                                    },
                                    "name": {
                                        "title": "Name",
                                        "type": "string"
                                    },
                                    "role": {
                                        "anyOf": [
                                            {
                                                "$ref": "#/$defs/Role"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null
                                    }
                                },
                                "required": [
                                    "name"
                                ],
                                "title": "Profile",
                                "type": "object"
                            },
                            "Role": {
                                "properties": {
                                    "name": {
                                        "title": "Name",
                                        "type": "string"
                                    },
                                    "description": {
                                        "title": "Description",
                                        "type": "string"
                                    },
                                    "is_static": {
                                        "default": false,
                                        "title": "Is Static",
                                        "type": "boolean"
                                    }
                                },
                                "required": [
                                    "name",
                                    "description"
                                ],
                                "title": "Role",
                                "type": "object"
                            }
                        },
                        "properties": {
                            "profile": {
                                "$ref": "#/$defs/Profile"
                            },
                            "chart_major_color": {
                                "anyOf": [
                                    {
                                        "pattern": "^#[0-9a-fA-F]{6}$",
                                        "type": "string"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ],
                                "default": null,
                                "title": "Chart Major Color"
                            },
                            "ai_backend_config": {
                                "$ref": "#/$defs/OpenAIBackendConfig"
                            }
                        },
                        "required": [
                            "profile",
                            "ai_backend_config"
                        ],
                        "title": "OpenAIBasicExamineeConfig",
                        "type": "object"
                    },
                    "obj_for_import": {
                        "obj": "OpenAIBasicExaminee",
                        "module": "general_mcq_examine.agents.openai_basic_examinee",
                        "source_file": null
                    }
                }
            ]
        },
        "evaluators_metadata": [
            {
                "cls_name": "RegexEvaluator",
                "description": "Regex evaluator (uses the specified regular expression to extract the answer, then compares it to the reference answer).",
                "config_schema": {
                    "$defs": {
                        "RegexEvalToolConfig": {
                            "properties": {
                                "regex_rules": {
                                    "default": [
                                        "\\b([A-D])\\b"
                                    ],
                                    "description": "Regex rules for extracting answers from the scene.",
                                    "items": {
                                        "type": "string"
                                    },
                                    "title": "Regex Rules",
                                    "type": "array"
                                },
                                "ignore_case": {
                                    "default": true,
                                    "description": "Whether to ignore case when matching regex rules and answers.",
                                    "title": "Ignore Case",
                                    "type": "boolean"
                                }
                            },
                            "title": "RegexEvalToolConfig",
                            "type": "object"
                        }
                    },
                    "properties": {
                        "regexEvalToolConfig": {
                            "$ref": "#/$defs/RegexEvalToolConfig"
                        }
                    },
                    "required": [
                        "regexEvalToolConfig"
                    ],
                    "title": "RegexEvaluatorConfig",
                    "type": "object"
                },
                "obj_for_import": {
                    "obj": "RegexEvaluator",
                    "module": "general_mcq_examine.metric_evaluators.regex_evaluator",
                    "source_file": null
                },
                "metrics": [
                    "examinee.answer_question.accurate"
                ]
            },
            {
                "cls_name": "SimpleEvaluator",
                "description": "A simple evaluator (determines whether the answer starts with the reference answer, ignoring case sensitivity).",
                "config_schema": {
                    "properties": {},
                    "title": "SimpleEvaluatorConfig",
                    "type": "object"
                },
                "obj_for_import": {
                    "obj": "SimpleEvaluator",
                    "module": "general_mcq_examine.metric_evaluators.simple_evaluator",
